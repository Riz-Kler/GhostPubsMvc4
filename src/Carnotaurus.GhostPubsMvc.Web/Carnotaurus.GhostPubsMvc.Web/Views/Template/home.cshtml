<!-- Subhead
    ================================================== -->
<header class="jumbotron subhead">
    <div class="container">
        <div class="span4 pull-left">
            <img width='200' height='200' onclick=" window.open('http://www.ghostpubs.com/haunted_pub/detail.html', '_self'); " src="~/Content/bootstrap/231/img/ghostpubs/padded-logo-125x125.png" alt="Ghost Pubs" title="Ghost Pubs" />
        </div>
        <div class="pull-left">
            <div>
                <h1>@Model.JumboTitle</h1>
                <p class="lead">
                    @Model.ArticleDescription
                </p>
            </div>
        </div>
    </div>
</header>


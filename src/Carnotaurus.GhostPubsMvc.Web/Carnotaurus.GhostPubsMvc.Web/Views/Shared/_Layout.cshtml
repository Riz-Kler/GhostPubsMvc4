    <script src="@Url.Content("~/content/bootstrap/231/js/bootstrap-collapse.js")"></script>
    <script src="@Url.Content("~/content/bootstrap/231/js/bootstrap-carousel.js")"></script>
    <script src="@Url.Content("~/content/bootstrap/231/js/bootstrap-typeahead.js")"></script>

                    <ul class="nav">
                        <li>
                            <a href="/">Home</a>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Ghost stories <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li><a href="/haunted_pub/detail.html">Stories by area</a></li>
                                <li><a href="/haunted_pub/submissions/detail.html">Submit a story</a></li>
                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">News <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a href="http://blog.ghostpubs.com/" target="blog">Recent news</a>
                                </li>
                                <li>
                                    <a href="/haunted_pub/promotions/detail.html">Promotions</a>
                                </li>
                                <li>
                                    <a href="/haunted_pub/competitions/detail.html">Competitions</a>
                                </li>
                            </ul>
                        </li>

                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Partners <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li><a href="/haunted_pub/partners/detail.html" title="Partners?">Partners</a></li>
                                <li>
                                    <a href="/haunted_pub/partnerships/detail.html">Partner with us</a>
                                </li>
                                <li>
                                    <a href="/haunted_pub/featuredpartner/detail.html">Featured partners</a>
                                </li>
                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Search<b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li style="width: 300px">
                                    <div class="gcse-container">
                                        <gcse:search></gcse:search>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a href="/haunted_pub/sitemap/detail.html">Sitemap</a>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">About us<b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a href="/haunted_pub/contributors/detail.html" title="Credits and contributions ...">Contributors</a>
                                </li>
                                <li>
                                    <a href="/haunted_pub/about/detail.html">About us</a>
                                </li>
                            </ul>
                        </li>

                    </ul>
                </div>
            </div>
        </div>
    </div>

    @RenderBody()


    <!-- Footer
        ================================================== -->
    <footer class="footer">
        <div class="container">
            <span class="label label-info">Designed and developed by <a href="http://www.carnotaurus.co.uk/" target="_blank" title="Carnotaurus Solutions website">Carnotaurus Solutions</a> </span>
        </div>
        <p>&nbsp;</p>
        <ul class="masthead-links">
            <li><a class="masthead-links-override" rel="nofollow" href="http://www.facebook.com/groups/ghostpubs"><i class="icon-facebook masthead-links-override"></i>fan page</a></li>
            <li><a class="masthead-links-override" rel="nofollow" href="https://twitter.com/ghost_pubs"><i class="icon-twitter-sign masthead-links-override"></i>twitter</a></li>
            <li><a class="masthead-links-override" rel="nofollow" href="http://www.linkedin.com/groups/Ghost-Pubs-4935170"><i class="icon-linkedin-sign masthead-links-override"></i>contact us</a></li>
            <li><a class="masthead-links-override" rel="nofollow" href="http://pinterest.com/ghostpubs/ghostpubscom"><i class="icon-pinterest-sign masthead-links-override"></i>our board</a></li>
            <li><a class="masthead-links-override" rel="nofollow" href="http://www.facebook.com/ghostpubspage"><i class="icon-facebook-sign masthead-links-override"></i>page</a></li>
        </ul>
        <p>&nbsp;</p>
        <p><a class="masthead-links-override" rel="nofollow" href="http://www.facebook.com/groups/ghostpubs">report this page | suggest your pub</a></p>

    </footer>
     
    @RenderSection("scripts", false)


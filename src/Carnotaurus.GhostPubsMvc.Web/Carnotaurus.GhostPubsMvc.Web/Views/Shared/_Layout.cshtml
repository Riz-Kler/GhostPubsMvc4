    <script src="@Url.Content("~/content/bootstrap/231/js/holder.js")"></script>
    <script src="@Url.Content("~/content/bootstrap/231/js/prettify.js")"></script>

    <script src="@Url.Content("~/content/bootstrap/231/js/application.js")"></script>

